// name: items(arr) artists(arr) name(object)
// album: items(arr de objects) name(obj) same for images

<div class="container">
  {{#each albums}}
  <div class="artists">
    <img src="{{this.images.0.url}}" width="200" height="250">
    <div class="info">
      <h4>{{this.name}}</h4>
      <a href="/album/tracks/{{this.id}}">View Tracks</a>
    </div>
  </div>
  {{/each}}
</div>